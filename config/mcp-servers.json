{
  "description": "User-scoped MCP servers to install via setup.sh",
  "servers": [
    {
      "name": "context7",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp", "--api-key", "$CONTEXT7_API_KEY"],
      "secrets": {
        "CONTEXT7_API_KEY": "op://Private/CONTEXT7_API_KEY/credential"
      }
    }
  ]
}
