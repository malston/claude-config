{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(python3:*)",
      "Bash(python:*)",
      "Bash(rm:*)",
      "Bash(ls:*)",
      "Bash(mkdir:*)",
      "Bash(chmod:*)",
      "Bash(sed:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(make test:*)",
      "mcp__git__git_commit",
      "Bash(curl:*)",
      "Bash(source:*)",
      "Bash(time make:*)",
      "Bash(gh issue list:*)",
      "Bash(npm test:*)",
      "Bash(git commit:*)",
      "Skill(code-reviewer)",
      "Bash(npm run test:coverage:*)",
      "Bash(npm run coverage:*)",
      "Bash(git add:*)",
      "Bash(HEADLESS=true npm run test:e2e:*)"
    ],
    "deny": [],
    "ask": [],
    "defaultMode": "acceptEdits"
  },
  "enabledPlugins": {
    "superpowers@superpowers-marketplace": true,
    "elements-of-style@superpowers-marketplace": true
  }
}
